      "Properties": {
        "ImageId": "ami-03d292eb4bc597178",
        "InstanceType": "t2.micro",
        "KeyName": "containers2",
        "SecurityGroupIds": ["sg-012ed60d267c38f6f"],
        "UserData": {
          "Fn::Base64": {
            "Fn::Join": [
              "",
              [
                "#!/bin/bash -xe\n",
                "# Change the hostname to serverNum20\n",
                "hostnamectl set-hostname serverNum20\n",
                "hostnamectl\n",
                "\n"
              ]
            ]
          }
        },
        "BlockDeviceMappings": [
          {
            "DeviceName": "/dev/sdm",
            "Ebs": {
              "VolumeType": "gp2",
              "DeleteOnTermination": "true",
              "VolumeSize": "8"
            }
          }
        ]
      }
    },
    "dnsNum20": {
      "Type": "AWS::Route53::RecordSet",
      "Properties": {
        "HostedZoneId": "Z3GEKPUSP8P6T5",
